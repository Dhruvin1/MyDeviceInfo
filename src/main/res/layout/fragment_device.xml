                android:layout_weight="1"
                android:paddingHorizontal="30dp"
                android:paddingVertical="5dp"
                android:text="TextView" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <TextView
